<?xml version="1.0" encoding="UTF-8"?>
<TEI xml:lang="fr" xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>{titre}</title>
        <author key="{auteur}"/>
      </titleStmt>
      <editionStmt>
        <edition>Centre André Chastel — ObTiC</edition>
        <respStmt>
          <name>{transcription}</name>
          <resp>Transcription</resp>
        </respStmt>
      </editionStmt>
      <publicationStmt>
        <publisher>Sorbonne Université, Centre André Chastel</publisher>
        <date when="2021"/>
        <idno>{filename}</idno><!-- A supprimer ?-->
        <availability status="restricted">
          <licence target="https://creativecommons.org/licenses/by-nc-sa/4.0/deed.fr"></licence>
        </availability>
      </publicationStmt>
      <seriesStmt>
        <title type="main">Correspondance d'Eugène Delacroix</title>
        <title type="sub">Hôtel Lambert</title><!-- optionnel -->
      </seriesStmt>
      <sourceDesc>
        <msDesc>
          <msIdentifier>
            <country>{cotepays}</country>
            <settlement>{cotepays}</settlement>
            <institution>{coteinstitution}</institution>
            <repository>{cotefonds}</repository>
            <idno>{cote}</idno>
          </msIdentifier>
          <physDesc>
            <objectDesc form="letter">
              <supportDesc>
                <extent>
                  <measure type="page">{pages}</measure><!-- optionnel -->
                </extent>
              </supportDesc>
            </objectDesc>
            <accMat>{accMat}</accMat><!-- optionnel -->
          </physDesc>
          <history>
            <provenance>{historique}</provenance><!-- optionnel -->
          </history>
        </msDesc>
        <listBibl>
          <bibl>{edition}</bibl><!-- optionnel -->
        </listBibl>
      </sourceDesc>
    </fileDesc>
    <profileDesc>
      <correspDesc>
        <correspAction type="sent">
          <persName key="{auteur}"/>
          <date when="{date}"/><!-- optionnel -->
          <placeName>{lieu}</placeName><!-- optionnel -->
        </correspAction>
        <correspAction type="received"><!-- optionnel -->
          <persName key="{destinataire}"/><!-- optionnel -->
        </correspAction>
        <correspContext>
          <ref target="{reponsea}" type="replyTo"/><!-- optionnel -->
        </correspContext>
      </correspDesc>
      <langUsage>
        <language ident="fr"/>
      </langUsage>
    </profileDesc>
  </teiHeader>
  <text>
    <body/>
  </text>
</TEI>
